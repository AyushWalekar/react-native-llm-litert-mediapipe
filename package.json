{
  "name": "react-native-llm-litert-mediapipe",
  "version": "1.0.0",
  "description": "React Native module for on-device LLM inference using LiteRT/MediaPipe (Gemma 3n)",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "react-native-llm-litert-mediapipe.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "clean": "rm -rf lib",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "open:ios": "xed example-bare/ios",
    "open:android": "open -a \"Android Studio\" example-bare/android"
  },
  "keywords": [
    "react-native",
    "mediapipe",
    "llm",
    "gemma",
    "gemma3n",
    "on-device",
    "ai",
    "inference",
    "android",
    "ios"
  ],
  "repository": "https://github.com/AyushWalekar/react-native-llm-litert-mediapipe",
  "bugs": {
    "url": "https://github.com/AyushWalekar/react-native-llm-litert-mediapipe/issues"
  },
  "author": "Ayush Walekar <walekarayush@gmail.com> (https://github.com/AyushWalekar)",
  "license": "MIT",
  "homepage": "https://github.com/AyushWalekar/react-native-llm-litert-mediapipe#readme",
  "devDependencies": {
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "react-native": "^0.79.6",
    "typescript": "^5.0.0",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0",
    "zod": "^3.24.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "platforms": {
    "ios": "14.0",
    "android": "24"
  }
}
