{
  "name": "react-native-llm-litert-mediapipe",
  "version": "0.1.0",
  "description": "React Native module for on-device LLM inference using Google MediaPipe and LiteRT-LM. Supports Gemma 3n with OpenAI-compatible chat API and multimodal input (images, audio).",
  "main": "lib/index-bare.js",
  "module": "lib/index-bare.js",
  "types": "lib/index-bare.d.ts",
  "react-native": "src/index-bare.ts",
  "source": "src/index-bare.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "react-native-llm-litert-mediapipe.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.bare.json",
    "clean": "rm -rf lib",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "open:ios": "xed example-bare/ios",
    "open:android": "open -a \"Android Studio\" example-bare/android"
  },
  "keywords": [
    "react-native",
    "mediapipe",
    "llm",
    "gemma",
    "gemma3n",
    "on-device",
    "ai",
    "inference",
    "android",
    "ios",
    "multimodal",
    "vision",
    "audio",
    "litertlm",
    "openai",
    "chat",
    "llama"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AyushWalekar/react-native-llm-litert-mediapipe"
  },
  "bugs": {
    "url": "https://github.com/AyushWalekar/react-native-llm-litert-mediapipe/issues"
  },
  "author": "Ayush Walekar <walekarayush@gmail.com> (https://github.com/AyushWalekar)",
  "license": "MIT",
  "homepage": "https://github.com/AyushWalekar/react-native-llm-litert-mediapipe#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "react-native": "^0.79.6",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "codegenConfig": {
    "name": "MediaPipeLlmSpec",
    "type": "modules",
    "jsSrcsDir": "src"
  },
  "platforms": {
    "ios": "14.0",
    "android": "24"
  }
}
